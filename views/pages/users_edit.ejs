<% include ../partials/header %>
  
  <form action ="/users/update" method ="post">
  <div class="form-group">
    <label for="">ID</label>
    <input type="number"
      class="form-control"value ="<%= users.user_id %>" name="user_id" user_id="" aria-describedby="helpId" placeholder="">
    <small user_id="helpId" class="form-text text-muted">User ID</small>
  </div>

<div class="form-group">
    <label for="">email</label> 
    <input type="text"
      class="form-control" value ="<%= users.email %>"name="email" user_id="" aria-describedby="helpId" placeholder="">
    <small user_id="helpId" class="form-text text-muted">Email</small>
  </div>

<div class="form-group">
    <label for="">password</label>
    <input type="text"
      class="form-control" value = "<%= users.password %>"name="password" user_id="" aria-describedby="helpId" placeholder="">
    <small user_id="helpId" class="form-text text-muted">Password</small>
  </div>
  <div class="form-group">
        <label for="">Time</label>
        <input type="text" class="form-control" value="<%= time %>" name="time" user_id="" aria-describedby="helpId" placeholder="" >
        <small user_id="helpId" class="form-text text-muted">Time</small>
      </div>
      
<button type="submit" class="btn btn-primary">Save</button>
<button type="submit" class="btn btn-dark" >Cancel</button>


</form>

 <% include ../partials/footer %>